		  <input class="new-description" name="descripton">
		  </input>
		  <input class="submit-button" type="submit" value="submit">
			</input>
		</form>
		<p>
			x: <span class="x-coord"></span> y: <span class="y-coord"></span>
		</p>
	</div>

	<div id="map-canvas">
	</div>

	<div class="address-panel">
		<form id="address-input-form">
			<input id="address-input" style="color: black;" name="address"></input>
			<input type="submit" value="address"></input>
		</form>
