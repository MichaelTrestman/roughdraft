<div class="container">
	<div class="spots">
		<h2> Spots </h2>
		<!-- <h3>Current Collection name</h3> -->
			<div id="create-new-spot-button" class="button">
				<h3>Create New Spot</h3>
			</div>

			<div class="button">
				<h3>List Archived Spots</h3>
			</div>

			<div id="spot-list">
				<ul>
				</ul>
			</div>

	</div>

	<div class="collections">
		<h2> Collections</h2>
		<ul>
			<li class="button">
				Create New Collection
			</li>
			<li class="button">
				My Collections
			</li>
			<li class="button">
				Public Collections
			</li>
		</ul>
